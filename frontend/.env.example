# Backend API URL
# For local development with Docker: http://localhost:8001
# For production: your deployed backend URL
REACT_APP_BACKEND_URL=http://localhost:8001

# WebSocket Configuration (for development)
WDS_SOCKET_PORT=443

# Feature Flags
REACT_APP_ENABLE_VISUAL_EDITS=false
ENABLE_HEALTH_CHECK=false

# Basic Auth (must match backend configuration)
REACT_APP_BASIC_AUTH_USERNAME=admin
REACT_APP_BASIC_AUTH_PASSWORD=your-secure-password
